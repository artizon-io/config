    "when": "!isMac"
  },
  {
    "key": "cmd+j",
    "command": "workbench.action.previousEditorInGroup",
    "when": "isMac"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.previousEditorInGroup",
    "when": "!isMac"
